{
  "private": false,
  "name": "goldjet-components",
  "description": "高捷二期组件",
  "license": "MIT",
  "version": "1.0.1",
  "keywords": [
    "goldjet",
    "components"
  ],
  "homepage": "",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "module": "dist/esm/index.js",
  "typings": "dist/esm/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "antd": "^4.22.8",
    "bssula": "^2.1.26",
    "dexie": "^3.2.2",
    "dexie-react-hooks": "^1.1.1",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "lodash": "^4.17.21",
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^13.0.0",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.184",
    "@umijs/fabric": "^2.8.1",
    "@umijs/test": "^3.0.5",
    "dumi": "^1.1.0",
    "father": "^4.0.0-rc.2",
    "gh-pages": "^4.0.0",
    "lint-staged": "^10.0.7",
    "lodash": "^4.17.21",
    "prettier": "^2.2.1",
    "yorkie": "^2.0.0"
  }
}
